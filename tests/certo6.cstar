int main(){
    int a;
    a = 10;
    while(a > 0){
        print(a);
        a = a - 1;
    }

    a = -10;
    while(a < 0){
        print(a);
        a = a + 1;
    }

    a = 10;
    while(a >= 0){
        print(a);
        a = a - 1;
    }

    a = -10;
    while(a <= 0){
        print(a);
        a = a + 1;
    }

    a = 10;
    while(a == 10){
        print(a);
        a = 9;
    }
    
    return 0;
}