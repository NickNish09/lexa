   12 $@2: %empty

   13 func_decl: TIPO ID '(' $@1 parm_tipos ')' $@2 '{' cod_blocks '}'

   14 $@3: %empty

   15 func_decl: TIPO ID '(' ')' '{' $@3 cod_blocks '}'

   16 parm_tipos: parm_tipos ',' TIPO ID
